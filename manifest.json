{
  "manifest_version": 2,

  "name": "One-click Aveece",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "content_scripts": [
   {
       "matches": ["http://*/*","https://*/*"],
       "js": ["jquery.js", "popup.js","cookies.js"]
   }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}